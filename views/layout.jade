!!! 5
html(lang="en")
  head
    script(type='text/javascript')
    - if (_debug)
      script(src="/js/jquery-1.5.1.js", type="text/javascript")
      script(src="/js/underscore.js", type="text/javascript")
    - else
      script(src="/js/jquery-1.5.1.min.js", type="text/javascript")
      script(src="/js/underscore-min.js", type="text/javascript")
    script(src="/js/app.js", type="text/javascript")
    link(rel='stylesheet', href="/css/app.css", type="text/css")
    link(rel='stylesheet', href="/css/blueprint/screen.css", type="text/css", media="screen, projection")
    link(rel='stylesheet', href="/css/blueprint/print.css", type="text/css", media="print")
    /if lt IE 8
      link(rel='stylesheet', href="css/blueprint/ie.css", type="text/css", media="screen, projection")
    title TITLE
  body
    - if (_debug)
      #content(class='showgrid container')
        !=body
    - else
      #content(class='container')
        !=body
